{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env DEBUG=api:server nodemon --exec ts-node ./src/bin/www.ts",
    "deploy": "tsc && pm2 stop all && pm2 delete all && pm2 start dist/bin/www.js",
    "test": "NODE_ENV=test npm run migrate:reset && NODE_ENV=test npm run migrate && node_modules/.bin/jest",
    "migrate": "cd ./src && sequelize db:migrate",
    "migrate:reset": "cd ./src && sequelize db:migrate:undo:all"
  },
