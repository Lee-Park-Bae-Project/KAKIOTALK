{
  "name": "@kakio/server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env DEBUG=api:server nodemon --exec ts-node --ignore ./src/__test__ -r tsconfig-paths/register ./src/bin/www.ts",
    "build": "tsc",
    "deploy": "yarn build && pm2 start dist/bin/www.js",
    "test": "NODE_ENV=test npm run migrate:reset && NODE_ENV=test npm run migrate && NODE_ENV=test npm run seed && node_modules/.bin/jest --detectOpenHandles --watchAll",
    "migrate": "cd ./src && sequelize db:migrate",
    "migrate:reset": "cd ./src && sequelize db:migrate:undo:all",
    "seed": "cd ./src && sequelize db:seed:all",
    "seed:reset": "cd ./src && sequelize db:seed:undo:all",
    "lint": "eslint --fix \"src/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "axios": "^0.19.2",
    "babel-core": "^6.26.3",
    "bluebird": "^3.7.2",
    "chalk": "^4.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "create-env": "^1.1.1",
    "cross-env": "^7.0.2",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "faker": "^4.1.0",
    "http-errors": "~1.6.3",
    "http-status": "^1.4.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "morgan": "^1.10.0",
    "mysql2": "^2.1.0",
    "node": "^13.12.0",
    "pidusage": "^2.0.20",
    "pm2": "^4.4.0",
    "sequelize": "^5.21.3",
    "shortid": "^2.2.15",
    "socket.io": "^2.3.0",
    "tsc-node": "0.0.3",
    "typescript": "^3.8.3",
    "uuid4": "^1.1.4"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.31",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/express-session": "^1.15.16",
    "@types/faker": "^4.1.11",
    "@types/http-errors": "^1.6.3",
    "@types/jest": "^25.2.1",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/morgan": "^1.9.0",
    "@types/nodemon": "^1.19.0",
    "@types/socket.io": "^2.1.4",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^25.4.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^25.4.0",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.8.2",
    "tsconfig-paths": "^3.9.0"
  }
}
